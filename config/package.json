{
	"name": "@dsmrt/axiom-config",
	"version": "0.2.3",
	"description": "Config library for axiom cli",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git@github.com:dsmrt/axiom.git",
		"directory": "config"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"lint": "biome lint",
		"lint:fix": "biome lint --fix",
		"test": "vitest run --coverage",
		"watch": "vitest watch --coverage",
		"build": "tsup --entry ./src/bin/axiom.ts --entry ./src/index.ts --format cjs,esm --dts --clean",
		"release": "pnpm run build && pnpm changeset publish"
	},
	"keywords": [
		"axiom",
		"config",
		"aws"
	],
	"author": {
		"name": "Damien Smrt",
		"url": "https://dsmrt.com"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@changesets/cli": "^2.27.11",
		"@types/node": "^20.17.10",
		"@types/yargs": "^17.0.33",
		"@vitest/coverage-v8": "^1.6.0",
		"ts-node": "^10.9.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"find-up": "^5.0.0",
		"yargs": "^17.7.2"
	}
}
