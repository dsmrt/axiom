{
    "name": "@dsmrt/axiom-cli",
    "version": "0.0.7-alpha.0",
    "description": "axiom cli for managing configs including secrets",
    "main": "lib/bin/axiom.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git@github.com:dsmrt/axiom.git",
        "directory": "cli"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "bin": {
        "axiom": "./lib/bin/axiom.js"
    },
    "scripts": {
        "lint": "eslint ./src/ --ext .ts",
        "lint:fix": "eslint ./src/ --ext .ts --fix",
        "test": "vitest run --coverage",
        "watch": "vitest watch --coverage",
        "build": "tsc -d -p ./tsconfig.json",
        "version:patch": "pnpm version patch"
    },
    "keywords": [
        "axiom",
        "config",
        "aws",
        "cli"
    ],
    "author": {
        "name": "Damien Smrt",
        "url": "https://dsmrt.com"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/inquirer": "^8.0.0",
        "@types/node": "^20.9.0",
        "@types/yargs": "^17.0.31",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "@vitest/coverage-v8": "^0.34.6",
        "aws-sdk-client-mock": "^3.0.0",
        "eslint": "^8.54.0",
        "prettier": "^3.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "vitest": "^0.34.6"
    },
    "dependencies": {
        "@aws-sdk/client-ssm": "^3.451.0",
        "@aws-sdk/client-sts": "^3.454.0",
        "@aws-sdk/credential-providers": "^3.454.0",
        "@aws-sdk/types": "^3.451.0",
        "@dsmrt/axiom-aws-sdk": "workspace:../aws-sdk",
        "@dsmrt/axiom-config": "workspace:../config",
        "chalk": "^4.1.2",
        "inquirer": "^8.0.0",
        "yargs": "^17.7.2"
    }
}
